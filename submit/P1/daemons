zebra=yes # yes|no (default: no) - enable/disable zebra
bgpd=yes # yes|no (default: no) - enable/disable bgpd
ospfd=yes # yes|no (default: no) - enable/disable ospfd
isisd=yes # yes|no (default: no) - enable/disable isisd
vtysh_enable=yes # yes|no (default: no) - enable vtysh config loading on start
zebra_options="  -A 127.0.0.1 -s 90000000" # zebra options (default: -A 127.0.0.1 -s 90000000) 
bgpd_options="   -A 127.0.0.1" # bgpd options (default: -A 127.0.0.1)
ospfd_options="  -A 127.0.0.1" # ospfd options (default: -A 127.0.0.1)
isisd_options="  -A 127.0.0.1" # isisd options (default: -A 127.0.0.1)
